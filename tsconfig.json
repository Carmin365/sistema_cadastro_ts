{
  "compilerOptions": {
    "target": "es2016",                                
    "module": "ESNext",                                
    "strict": true,                                  
    "strictNullChecks": true,                                  
    "noImplicitAny": true,                        
    "outDir": "./dist",                              
    "rootDir": "./src",                          
    "moduleResolution": "node",
    "esModuleInterop": true,                  
    "forceConsistentCasingInFileNames": true, 
    }, 
    "include": ["src"],
    "exclude": ["node_modules", "dist"]
}
